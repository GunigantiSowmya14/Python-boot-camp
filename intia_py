class Myclass:
    def __init__(self,a,b):
        self,a=0
        self,b=0
    def add(a,b):
        return(a+b)
    def sub(a,b):
        return(a-b)
    def mul(a,b):
        return(a*b)
    def div(a,b):
        return(a/b)
    def mod(a,b):
        return(a%b)
obj1=Myclass.add(2,3)
print(obj1)    
obj2=Myclass.sub(2,3)
print(obj2)